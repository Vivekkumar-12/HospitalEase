{
  "version": 2,
  "public": ".",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*\\.html?)$", "headers": { "Content-Type": "text/html; charset=utf-8" } },
    { "src": "/(.*\\.php)$", "headers": { "Content-Type": "text/html; charset=utf-8" } },
    { "src": "/(.*)", "dest": "/index.php" }
  ]
}
